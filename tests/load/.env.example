# Firebase Functions Configuration
FIREBASE_FUNCTIONS_URL=https://europe-west3-avalo-app.cloudfunctions.net
FIREBASE_PROJECT_ID=avalo-app

# Test User Authentication
# Generate tokens using: firebase login:ci or get from Firebase Console
TEST_USER_TOKEN=your-firebase-auth-id-token-here
TEST_USER_1_TOKEN=test-user-1-token
TEST_USER_2_TOKEN=test-user-2-token

# Load Test Configuration
MAX_VUS=1000
TEST_DURATION=3m
RAMP_UP_TIME=30s

# Enable verbose logging
DEBUG=false